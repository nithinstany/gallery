<h1>Editing gallery</h1>

<% form_for([:admin,@gallery],:html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>
    <%= render :partial => "form", :object => f %>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', admin_gallery_path(@gallery) %> |
<%= link_to 'Back', admin_galleries_path %>

